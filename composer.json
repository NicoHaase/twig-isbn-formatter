{
  "name": "nicohaase/twig-isbn-formatter",
  "type": "library",
  "description": "ISBN formatting extension for Twig",
  "keywords": [
    "isbn",
    "symfony",
    "bundle",
    "twig",
    "twig extension"
  ],
  "homepage": "https://github.com/NicoHaase/twig-isbn-formatter",
  "license": "MIT",
  "authors": [
    {
      "name": "Nico Haase",
      "homepage": "https://www.nicohaase.de"
    }
  ],
  "require": {
    "php": "^7.4|^8.1",
    "nicebooks/isbn": "^0.3|^0.4",
    "twig/twig": "^2.15|^3.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6"
  },
  "autoload": {
    "psr-4": {
      "NicoHaase\\TwigIsbnFormatter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NicoHaase\\TwigIsbnFormatter\\Tests\\": "test/"
    }
  }
}
